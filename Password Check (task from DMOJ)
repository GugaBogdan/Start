#https://dmoj.ca/problem/wc17c3j3

password = input("Enter your password: ")

lowercase = 0
uppercase = 0
digits = 0

for i in password:
    if password.islower():
        lowercase += 1
    elif password.isupper():
        uppercase += 1
    elif password.isdigit():
        digits += 1

if lowercase <= 2 or uppercase <= 1 or digits <= 0:
    print("Invalid")
elif len(password) <= 7 or len(password) >= 13:
    print("Invalid")
else:
    print("Valid")

